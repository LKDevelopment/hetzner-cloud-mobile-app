{
  "name": "hetzner-cloud-app",
  "version": "1.2.0",
  "author": "Lukas KÃ¤mmerling",
  "homepage": "https://lk-development.de/",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "bnr dev",
    "build": "bnr build",
    "build-dev": "bnr build-dev",
    "test": "bnr test",
    "test:watch": "bnr test-watch",
    "e2e": "npm run e2e-update && npm run e2e-test",
    "e2e-test": "protractor ./test-config/protractor.conf.js",
    "e2e-update": "webdriver-manager update --standalone false --gecko false",
    "ionic:serve": "ionic-app-scripts serve",
    "ionic:build": "ionic-app-scripts build",
    "ios:dev": "bnr ios:dev",
    "ios:release": "bnr ios:release",
    "ios:release:ci": "bnr ios:release:ci",
    "android:dev": "bnr android:dev",
    "android:release": "bnr android:release",
    "cz": "git-cz",
    "docs": "typedoc --options typedoc.json --exclude '**/*+(e2e|spec|index).ts' ./src/",
    "release": "standard-version --no-verify",
    "push": "git push --follow-tags origin master",
    "lint": "tslint \"src/**/*.ts\" --project ./tsconfig.json",
    "scss-lint": "scss-lint",
    "outdated": "npm outdated --depth 0",
    "post-install": "webdriver-manager update --standalone",
    "prepush": "npm test",
    "commitmsg": "validate-commit-msg",
    "compodoc": "./node_modules/.bin/compodoc src/ -p tsconfig.json -n Hetzner-Cloud-Mobile",
    "e2e:loc": "LANG='de' node test-config/protractor.server.js && LANG='de' node test-config/protractor.server.js"
  },
  "betterScripts": {
    "dev": {
      "command": "ionic-app-scripts serve",
      "env": {
        "NODE_ENV": "development",
        "PLATFORM": "web"
      }
    },
    "build": {
      "command": "ionic-app-scripts build --prod --tsconfig ./tsconfig.json",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "build-dev": {
      "command": "ionic-app-scripts build --dev",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "test": {
      "command": "karma start ./test-config/karma.conf.js --single-run",
      "env": {
        "NODE_ENV": "test"
      }
    },
    "test-watch": {
      "command": "karma start --auto-watch --no-single-run",
      "env": {
        "NODE_ENV": "test",
        "NO_COVERAGE": true
      }
    },
    "ios:dev": {
      "command": "ionic cordova run ios --livereload",
      "env": {
        "NODE_ENV": "development",
        "PLATFORM": "ios"
      }
    },
    "ios:release": {
      "command": "ionic cordova build ios --prod --release",
      "env": {
        "NODE_ENV": "production",
        "PLATFORM": "ios"
      }
    },
    "ios:release:ci": {
      "command": "npm run build",
      "env": {
        "NODE_ENV": "production",
        "PLATFORM": "ios"
      }
    },
    "android:dev": {
      "command": "ionic cordova run android --livereload",
      "env": {
        "NODE_ENV": "development",
        "PLATFORM": "android"
      }
    },
    "android:release": {
      "command": "ionic cordova build android --release",
      "env": {
        "NODE_ENV": "production",
        "PLATFORM": "android"
      }
    }
  },
  "dependencies": {
    "@angular/animations": "^5.0.3",
    "@angular/common": "5.0.3",
    "@angular/compiler": "5.0.3",
    "@angular/compiler-cli": "5.0.3",
    "@angular/core": "5.0.3",
    "@angular/forms": "5.0.3",
    "@angular/http": "5.0.3",
    "@angular/platform-browser": "5.0.3",
    "@angular/platform-browser-dynamic": "5.0.3",
    "@ionic-native/app-version": "^4.5.3",
    "@ionic-native/barcode-scanner": "^4.5.3",
    "@ionic-native/core": "4.4.0",
    "@ionic-native/fingerprint-aio": "^4.5.3",
    "@ionic-native/globalization": "^4.5.3",
    "@ionic-native/in-app-browser": "^4.5.3",
    "@ionic-native/keyboard": "^4.5.3",
    "@ionic-native/onesignal": "^4.5.3",
    "@ionic-native/splash-screen": "4.4.0",
    "@ionic-native/status-bar": "4.4.0",
    "@ionic/pro": "^1.0.20",
    "@ionic/storage": "^2.1.3",
    "@lkdevelopment/ngx-qrcode": "git://github.com/LKDevelopment/ngx-qrcode#master",
    "@ngx-translate/core": "^9.1.1",
    "@ngx-translate/http-loader": "^2.0.1",
    "@novnc/novnc": "^1.0.0",
    "better-npm-run": "^0.1.0",
    "chart.js": "^2.7.2",
    "chartist": "^0.11.0",
    "cordova-android": "6.4.0",
    "cordova-ios": "4.5.4",
    "cordova-plugin-add-swift-support": "^1.7.1",
    "cordova-plugin-app-version": "^0.1.9",
    "cordova-plugin-camera": "^4.0.2",
    "cordova-plugin-device": "^1.1.7",
    "cordova-plugin-fingerprint-aio": "^1.3.4",
    "cordova-plugin-globalization": "^1.0.9",
    "cordova-plugin-inappbrowser": "^2.0.2",
    "cordova-plugin-ionic-keyboard": "^2.0.5",
    "cordova-plugin-ionic-webview": "^1.1.16",
    "cordova-plugin-splashscreen": "^4.1.0",
    "cordova-plugin-statusbar": "^2.4.1",
    "cordova-plugin-whitelist": "^1.3.3",
    "cordova-sqlite-storage": "^2.2.1",
    "emulator": "0.1.0",
    "ionic-angular": "3.9.2",
    "ionic-plugin-keyboard": "^2.2.1",
    "ionic-tooltips": "^2.0.1",
    "ionicons": "3.0.0",
    "ng-animate": "^0.3.1",
    "ng-chartist": "^1.1.1",
    "ng2-charts": "^1.6.0",
    "onesignal-cordova-plugin": "^2.3.1",
    "parse-url": "^3.0.2",
    "phonegap-plugin-barcodescanner": "^7.0.2",
    "pretty-bytes": "^4.0.2",
    "rxjs": "5.5.2",
    "sw-toolbox": "3.6.0",
    "web-animations-js": "^2.3.1",
    "xml2js": "^0.4.19",
    "zone.js": "0.8.18"
  },
  "devDependencies": {
    "@compodoc/compodoc": "^1.0.8",
    "@ionic/app-scripts": "3.1.8",
    "@types/chartist": "^0.9.38",
    "@types/jasmine": "^2.8.6",
    "@types/node": "^9.4.6",
    "angular2-template-loader": "^0.6.2",
    "html-loader": "^0.5.5",
    "ionic-mocks": "^1.3.0",
    "istanbul-instrumenter-loader": "^3.0.0",
    "jasmine": "2.99",
    "jasmine-core": "2.99",
    "jasmine-spec-reporter": "^4.2.1",
    "karma": "^2.0.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage-istanbul-reporter": "^1.4.1",
    "karma-jasmine": "^1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.13",
    "null-loader": "^0.1.1",
    "protractor": "^5.3.0",
    "serve-static": "1.13.1",
    "ts-loader": "^3.5.0",
    "ts-node": "^5.0.1",
    "typescript": "2.4.2"
  },
  "description": "A mobile App for the Hetzner Cloud.",
  "cordova": {
    "plugins": {
      "cordova-sqlite-storage": {},
      "cordova-plugin-whitelist": {},
      "cordova-plugin-device": {},
      "cordova-plugin-splashscreen": {},
      "cordova-plugin-ionic-webview": {},
      "cordova-plugin-ionic-keyboard": {},
      "cordova-plugin-inappbrowser": {},
      "cordova-plugin-app-version": {},
      "cordova-plugin-fingerprint-aio": {},
      "onesignal-cordova-plugin": {},
      "cordova-plugin-camera": {},
      "phonegap-plugin-barcodescanner": {}
    },
    "platforms": [
      "android"
    ]
  },
  "processEnv": [
    "TEST_API_KEY",
    "CI",
    "SCREENSHOTS",
    "LANG"
  ]
}
