    <icon height="87" src="resources/ios/icon/Icon-87.png" width="87"/>
    <icon height="1024" src="resources/ios/icon/Icon-1024.png" width="1024"/>
    <splash height="1136" src="resources/ios/splash/Default-568h@2x~iphone.png" width="640"/>
    <splash height="1334" src="resources/ios/splash/Default-667h.png" width="750"/>
    <splash height="2208" src="resources/ios/splash/Default-736h.png" width="1242"/>
    <splash height="1242" src="resources/ios/splash/Default-Landscape-736h.png" width="2208"/>
    <splash height="1536" src="resources/ios/splash/Default-Landscape@2x~ipad.png" width="2048"/>
    <splash height="2048" src="resources/ios/splash/Default-Landscape@~ipadpro.png" width="2732"/>
    <splash height="768" src="resources/ios/splash/Default-Landscape~ipad.png" width="1024"/>
    <splash height="2048" src="resources/ios/splash/Default-Portrait@2x~ipad.png" width="1536"/>
    <splash height="2732" src="resources/ios/splash/Default-Portrait@~ipadpro.png" width="2048"/>
    <splash height="1024" src="resources/ios/splash/Default-Portrait~ipad.png" width="768"/>
    <splash height="960" src="resources/ios/splash/Default@2x~iphone.png" width="640"/>
    <splash height="480" src="resources/ios/splash/Default~iphone.png" width="320"/>
    <splash height="2732" src="resources/ios/splash/Default@2x~universal~anyany.png" width="2732"/>
    <config-file mode="add" parent="ITSAppUsesNonExemptEncryption" target="*-Info.plist">
      <false/>
    </config-file>
    <edit-config file="*-Info.plist" mode="merge" target="NSCameraUsageDescription">
      <string>To scan QR-Codes Generated from other instances of the app</string>
    </edit-config>
    <edit-config file="*-Info.plist" mode="merge" target="NSPhotoLibraryUsageDescription">
      <string>To push scan QR-Codes Generated from other instances of the app</string>
    </edit-config>
  </platform>
  <plugin name="cordova-sqlite-storage" spec="~2.2.0"/>
  <plugin name="cordova-plugin-whitelist" spec="~1.3.1"/>
  <plugin name="cordova-plugin-ionic-webview" spec="^1.1.11"/>
  <plugin name="cordova-plugin-ionic-keyboard" spec="^2.0.5"/>
  <plugin name="cordova-plugin-inappbrowser" spec="^2.0.2"/>
  <plugin name="cordova-plugin-app-version" spec="^0.1.9"/>
  <plugin name="cordova-plugin-advanced-http" spec="^1.11.1"/>
  <plugin name="onesignal-cordova-plugin" spec="^2.3.1"/>
  <plugin name="cordova-plugin-camera" spec="^4.0.2"/>
  <plugin name="phonegap-plugin-barcodescanner" spec="^7.0.2"/>
  <allow-navigation href="http://192.168.178.24:8100"/>
  <allow-navigation href="*"/>
  <access origin="wss:///*"/>
  <access origin="https://api.hetzner.cloud"/>
  <access origin="https://robot-ws.your-server.de"/>
  <plugin name="cordova-plugin-apprate" spec="^1.3.0"/>
  <plugin name="cordova-plugin-network-information" spec="^2.0.1"/>
  <allow-navigation href="http://192.168.178.87:8100"/>
  <engine name="android" spec="6.4.0"/>
  <plugin name="cordova-plugin-device" spec="^2.0.1"/>
  <plugin name="cordova-plugin-ionic" spec="^3.1.3">
    <variable name="APP_ID" value="359b3ec5"/>
    <variable name="CHANNEL_NAME" value="Beta"/>
    <variable name="UPDATE_METHOD" value="none"/>
    <variable name="WARN_DEBUG" value="true"/>
    <variable name="UPDATE_API" value="https://api.ionicjs.com"/>
    <variable name="MAX_STORE" value="2"/>
  </plugin>
  <plugin name="cordova-plugin-splashscreen" spec="^5.0.2"/>
  <plugin name="cordova-plugin-ignore-lint-translation" spec="^0.0.1"/>
  <plugin name="cordova-plugin-fingerprint-aio" spec="~1.3.6-2"/>
</widget>
